#
# AirDrop anchor point.
#
anchor "200.AirDrop/*"

#
# Application Firewall anchor point.
#
anchor "250.ApplicationFirewall/*"
anchor "910.Custom/*"
load anchor "910.Custom" from "/etc/pf.anchors/910.Custom"

